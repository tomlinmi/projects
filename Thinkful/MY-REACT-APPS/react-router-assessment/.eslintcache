ner\">\n      <PostsNav />\n      <div className=\"border p-4 h-100 d-flex flex-column\">\n        <h2 className=\"mb-3\">{user.name}</h2>\n        <ul className=\"nav nav-tabs\">\n          <li className=\"nav-item\">\n          <Link to={`${url}`}>Profile</Link>\n          </li>\n          <li className=\"nav-item\">\n          <Link to={`${url}/posts`}>Posts</Link> \n          </li>\n        </ul>\n\n        {user.id ? (\n          <div className=\"p-4 border border-top-0\">\n            <Switch>\n              <Route path = {`${url}/posts`}>\n            <PostList posts={user.posts} />\n                </Route>\n              <Route exact path = {`${url}`}>\n            <UserProfile user={user} />\n              </Route>\n              </Switch>\n          </div>\n        ) : (\n          <div className=\"p-4 border border-top-0\">\n            <p>Loading...</p>\n          </div>\n        )}\n      </div>\n    </section>\n  );\n};\n\nexport default User;\n","C:\\Users\\mntom\\projects\\Thinkful\\MY-REACT-APPS\\react-router-assessment\\src\\common\\ErrorMessage.js",[],"C:\\Users\\mntom\\projects\\Thinkful\\MY-REACT-APPS\\react-router-assessment\\src\\home\\Card.js",["71"],"import React from \"react\";\nimport{ Link, Route, Switch} from \"react-router-dom\";\nimport User from \"../user/User\";\nimport PostList from \"../user/PostList\";\n\n/*\n  TODO: Change the link below to go to the user route, using the user's ID.\n  /users/:userId\n*/\n\nexport const Card = ({ user = { posts: [] } }) => (\n\n  \n  <article className=\"col-12 col-md-6 col-xl-3 my-2 align-self-stretch\">\n    <div className=\"border p-4 h-100 d-flex flex-column\">\n      <h2 className=\"font-weight-lighter flex-fill\">\n    <Link to={`/users/${user.id}`}>{user.name}</Link>\n      </h2>\n       <Route path={`/users/:${user.id}`}> \n         <User user={user}/> \n      </Route> \n      <Link className=\"mt-2\" to={`/users/${user.id}/posts`}>{user.posts.length} Posts Â»</Link> \n      <Route path=